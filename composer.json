{
    "name": "bliq-mx/bliq-api-php",
    "version": "1.5.1",
    "description": "Biblioteca para conectar con la API de timbrado de Reeply",
    "keywords": ["sat", "cfdi", "timbrado"],
    "homepage": "https://github.com/bliq-mx/bliq-stamp-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Reeply",
            "email": "info@reeply.mx",
            "homepage": "https://www.reeply.mx/"
        }
    ],
    "support": {
        "source": "https://github.com/bliq-mx/bliq-stamp-api",
        "issues": "https://github.com/bliq-mx/bliq-stamp-api/issues"
    },
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
